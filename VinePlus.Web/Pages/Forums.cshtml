@page "{p=1}"
@using VinePlus.Web.Pages
@model VinePlus.Web.Pages.Forums

@{
    Layout= "Shared/_Layout";
    ViewData[Keys.Title]     = "Recent Topics";
    ViewData[Keys.Headings]  = "Recent discussions on Vine";
    ViewData[Keys.Highlight] = MainHighlight.Vine;
}

<div id="thread">
    @await Html.PartialAsync("Shared/Components/Navigation", (Model.NavRecord, Model.PageDelegate()))
    @await Html.PartialAsync("Shared/Components/ForumItems", (Model.Entities , Model.GetThreadLink()))
    @await Html.PartialAsync("Shared/Components/Navigation", (Model.NavRecord, Model.PageDelegate()))
</div>