@page
@using VinePlus.Web.Pages
@model VinePlus.Web.Pages.Thread

@{
    Layout = "Shared/_Layout";
    ViewData[Keys.Title]    = Model.ThreadTitle;
    ViewData[Keys.Headings] = Model.ThreadTitle;
}

@await Html.PartialAsync("Shared/Components/Navigation", (Model.NavRecord, Model.PageDelegate()))
@await Html.PartialAsync("Shared/Components/ThreadItems", Model.Entities)
@await Html.PartialAsync("Shared/Components/ThreadFooter", $"https://comicvine.gamespot.com{Model.OriginalThread}")
@await Html.PartialAsync("Shared/Components/Navigation", (Model.NavRecord, Model.PageDelegate()))
